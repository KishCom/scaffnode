{
  "name": "scaffnode",
  "author": "Andrew Kish",
  "version": "0.16.8",
  "private": true,
  "scripts": {
    "start": "clear;cd frontend;grunt start_app;cd ..;NODE_ENV=dev nodemon app.js | bunyan",
    "start.live": "clear;cd frontend;grunt build;cd ..;NODE_ENV=live nodemon app.js | bunyan",
    "postinstall": "cd frontend;npm install;cd ..;",
    "test": "cd frontend;grunt test;cd ..;NODE_ENV=test mocha tests/*_tests.js;karma start app_karma.conf.js --log-level debug --single-run"
  },
  "dependencies": {
    "accepts": "1.3.x",
    "body-parser": "1.15.x",
    "bunyan": "1.8.x",
    "connect-redis": "3.1.x",
    "cookie-parser": "1.4.x",
    "escape-html": "1.0.x",
    "express": "4.14.x",
    "express-session": "1.14.x",
    "hiredis": "0.5.x",
    "hpp": "0.2.x",
    "i18n": "0.8.x",
    "less": "2.7.x",
    "less-middleware": "2.2.x",
    "lodash": "4.13.x",
    "multer": "1.1.x",
    "nodemon": "1.9.x",
    "nunjucks": "2.4.x",
    "nunjucks-date-filter": "0.1.x",
    "redis": "2.6.x",
    "validator": "5.4.x"
  },
  "devDependencies": {
    "chai": "3.4.x",
    "eslint": "3.1.1",
    "karma": "1.1.x",
    "karma-chrome-launcher": "^1.0.x",
    "karma-firefox-launcher": "^1.0.x",
    "karma-mocha": "1.0.x",
    "karma-phantomjs-launcher": "1.0.x",
    "mocha": "2.5.x",
    "phantomjs-prebuilt": "2.1.x",
    "supertest": "1.2.x"
  }
